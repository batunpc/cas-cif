[
  {
    "routePaths": ["/", "/login-redirect", "/unauthorized_idir"],
    "isProtected": false
  },
  {
    "routePaths": ["/admin(.*)"],
    "isProtected": true,
    "allowedRoles": ["cif_admin", "Realm Administrator"]
  },
  {
    "routePaths": ["/cif"],
    "isProtected": true,
    "allowedRoles": ["cif_internal"]
  },
  {
    "routePaths": ["/cif/(.*)"],
    "isProtected": true,
    "allowedRoles": ["cif_internal", "cif_admin", "Realm Administrator"]
  }
]
